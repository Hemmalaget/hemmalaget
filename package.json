{
  "name": "hemmalaget",
  "description": "Homepage for Hemmalaget",
  "author": "Daniel Str√∂m",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Hemmalaget/hemmalaget"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "node server.dev.js",
    "prebuild": "rm -rf docs/",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors",
    "test": "jest",
    "deploy": "./scripts/deploy.fish",
    "prettify": "prettier --no-semi --write app/**/*.js app/**/*.css"
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "bower_components", "shared"],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileTransformer.js"
    }
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.28.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.14.0",
    "file-loader": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.1.0",
    "jest": "^20.0.4",
    "postcss": "^6.0.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^6.1.0",
    "prettier": "^1.10.2",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "yarn": "^0.24.5"
  }
}
