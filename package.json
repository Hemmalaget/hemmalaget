{
  "name": "hemmalaget",
  "description": "Homepage for Hemmalaget",
  "author": "Daniel Str√∂m",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Hemmalaget/hemmalaget"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "node server.dev.js",
    "prebuild": "rm -rf dist/",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors",
    "test": "jest",
    "prettify": "prettier --no-semi --write app/**/*.js app/**/*.css"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "setupFiles": [
      "<rootDir>/setup-jest.js"
    ]
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.14.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.1.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "postcss": "^6.0.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "4.0.0",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
